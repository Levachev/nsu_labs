import compile_util/parse;
import compile_util/analys;
import compile_util/transl;

main() {
    program = parse("#include example.nemo");
    result = analyse(program);
    if (result == Success()) {
        ret = setFileContent("./example.out", translate(program));
        if (!ret) {
            println("Cannot create output file");
        }
    } else {
        println(analysisResult2s(result));
    }
    quit(0);
}