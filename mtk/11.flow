    equalExpr(e1:ArExpr,e2:ArExpr)->bool{
        switch (e1) {
            ArSum(lhs1, rhs1): {
                switch(e2){
                    ArSum(lhs2,rhs2): equalExpr(lhs1, lhs2) && equalExpr(rhs1, rhs2);
                    default: false;
                }
            }
            ArDif(lhs1, rhs1): {
                switch(e2){
                    ArDif(lhs2,rhs2): equalExpr(lhs1, lhs2) && equalExpr(rhs1, rhs2);
                    default: false;
                }
            }
            ArMul(lhs1, rhs1): {
                switch(e2){
                    ArMul(lhs2,rhs2): equalExpr(lhs1, lhs2) && equalExpr(rhs1, rhs2);
                    default: false;
                }
            }
            ArDiv(lhs1, rhs1): {
                switch(e2){
                    ArDiv(lhs2,rhs2): equalExpr(lhs1, lhs2) && equalExpr(rhs1, rhs2);
                    default: false;
                }
            }
            ArVar(val1): {
                // switch(e2){
                //     ArVar(val2): val1==val2;
                //     default: false;
                // }
                true;
            }
            ArInt(val1): {
                switch(e2){
                    ArInt(val2): val1==val2;
                    default: false;
                }
            }
            ArUnar(val1): {
                switch(e2){
                    ArUnar(val2): equalExpr(val1, val2);
                    default: false;
                }
            }
        }
    }

