    diffDrob(val1:Pair<Viraj, Viraj>, val2:Pair<Viraj, Viraj>) ->Pair<Viraj, Viraj>{
        v1_ch = val1.first;
        v1_zn = val1.second;

        v2_ch = val2.first;
        v2_zn = val2.second;

        if(v1_zn == v2_zn){
            Pair(diffVir(v1_ch,v2_ch), v1_zn);
        } else{
            tmp = Pair(diffVir(mulVir(v1_ch, v2_zn), mulVir(v2_ch, v1_zn)), mulVir(v1_zn, v2_zn));
            // println("diff");
            // printVir(tmp.first);
            tmp;
        }        
    }

