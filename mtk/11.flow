    changePeremm(e:ArExpr, map:HashMap<string,string>)->ArExpr{
        switch (e) {
            ArSum(lhs, rhs): ArSum(changePeremm(rhs,map), changePeremm(lhs,map));
            ArDif(lhs, rhs): ArDif(changePeremm(lhs,map), changePeremm(rhs,map));
            ArMul(lhs, rhs): ArMul(changePeremm(lhs,map), changePeremm(rhs,map));
            ArDiv(lhs, rhs): ArDiv(changePeremm(lhs,map), changePeremm(rhs,map));
            ArInt(val): ArInt(s2i(getMatch(i2s(val),map)));
            ArVar(val): ArVar(getMatch(val,map));
            ArUnar(val): ArUnar(changePeremm(val,map));
        }
    }

    changeExprs(val:ArExpr, map:HashMap<ArExpr, ArExpr>)->ArExpr{
    foldHashMap(map, val, \k,v,ret->{
        map2=makeHashMap(\s->1);
        if(equalExpr(k, val) && equalPeremenn(k, val,map2)){
            changePeremm(v,map2);
        } else{
            ret;
        }
    })
   }

   allchangeExprs(val:ArExpr, map:HashMap<ArExpr, ArExpr>)->ArExpr{
    tmp = changeExprs(val,map);
    map2=makeHashMap(\s->1);
    if(equalExpr(val, tmp) && equalPeremenn(val, tmp, map2)){
        changePeremm(tmp,map2);
        //tmp;
    } else{
        changeExprs(tmp,map);
    }
   }

    ar2drob2(e : ArExpr, map:HashMap<ArExpr, ArExpr>) -> ArExpr{
        switch (e) {
            ArSum(lhs, rhs): {
                e1 = ar2drob2(lhs,map);
                e2 = ar2drob2(rhs,map);
                allchangeExprs(ArSum(e2, e1),map);
            }
            ArDif(lhs, rhs): {
                e1 = ar2drob2(lhs,map);
                e2 = ar2drob2(rhs,map);
                allchangeExprs(ArDif(e2, e1),map);
            }
            ArMul(lhs, rhs): {
                e1 = ar2drob2(lhs,map);
                e2 = ar2drob2(rhs,map);

                tmp=changeExprs(ArMul(e2, e1),map);
                
                tmp;
            };
            ArDiv(lhs, rhs): {
                e1 = ar2drob2(lhs,map);
                e2 = ar2drob2(rhs,map);
                allchangeExprs(ArDiv(e1, e2),map);
            }
            ArInt(val):{
                allchangeExprs(ArInt(val),map);
            } 
            ArVar(val): {
                allchangeExprs(ArVar(val),map);
            }
            ArUnar(val): {
                e1 = ArInt(-1);
                e2 = ar2drob2(val,map);
                allchangeExprs(ArMul(e2, e1),map);
            }
        }
    }

