    s2ar(str : string) -> ArExpr{
        e_gr = "#include grammar3.lingo";
        parsic(
            compilePegGrammar(e_gr),
            str,
            SemanticActions(setTree(defaultPegActions.t, "CreateString", \s ->s[0]))
        )
    }

    
