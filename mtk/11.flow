    Zamena(val:ArExpr, map:HashMap<ArExpr, ArExpr>)->ArExpr{
        tmp = ar2drob2(val, map);
        map2=makeHashMap(\s->1);
        if(equalExpr(tmp,val) && equalPeremenn(tmp,val, map2)){
            changePeremm(tmp,map2);
            //tmp;
        } else{
            ar2drob2(tmp, map);
        }
    }

    not_ar2(e : ArExpr) -> string {
        switch (e) {
            ArSum(lhs, rhs): "("+not_ar2(rhs)+"+"+not_ar2(lhs)+")";
            ArDif(lhs, rhs): "("+not_ar2(lhs)+"-"+not_ar2(rhs)+")";
            ArMul(lhs, rhs): "("+not_ar2(rhs)+"*"+not_ar2(lhs)+")";
            ArDiv(lhs, rhs): "("+not_ar2(lhs)+"/"+not_ar2(rhs)+")";
            ArVar(val): val;
            ArInt(val): i2s(val);
            ArUnar(val): "(-1*"+not_ar2(val)+")";
        }
    }


