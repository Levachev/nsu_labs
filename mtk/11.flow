    getExprPerem(e:ArExpr)->[string]{
        switch (e) {
            ArSum(lhs, rhs): concat(getExprPerem(rhs), getExprPerem(lhs));
            ArDif(lhs, rhs): concat(getExprPerem(rhs), getExprPerem(lhs));
            ArMul(lhs, rhs): concat(getExprPerem(rhs), getExprPerem(lhs));
            ArDiv(lhs, rhs): concat(getExprPerem(rhs), getExprPerem(lhs));
            ArInt(val): arrayPush([], i2s(val));
            ArVar(val): arrayPush([], val);
            ArUnar(val): concat(arrayPush([],i2s(-1)), getExprPerem(val));
        }
    }

